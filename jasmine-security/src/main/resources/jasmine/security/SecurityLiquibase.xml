            </column>
            <column name="tenant_id" type="bigint" remarks="租户ID">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="jasmine_20220128222309" author="mh.z">
        <createTable tableName="sec_function_permission" remarks="关联功能和权限">
            <column name="id" type="bigint" remarks="主键id">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_sec_function_permission"/>
