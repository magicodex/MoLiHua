        </createTable>
    </changeSet>

    <changeSet id="jasmine_20220128222311" author="mh.z">
        <createTable tableName="sec_permission_set_permission" remarks="关联权限集和权限">
            <column name="id" type="bigint" remarks="主键id">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_sec_permission_set_perm"/>
            </column>
            <column name="permission_set_id" type="bigint" remarks="权限集ID">
                <constraints nullable="false"/>
            </column>
            <column name="permission_id" type="bigint" remarks="权限ID">
                <constraints nullable="false"/>
