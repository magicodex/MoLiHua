        </createTable>
    </changeSet>

    <changeSet id="jasmine_20220128222312" author="mh.z">
        <createTable tableName="sec_menu_function" remarks="关联菜单和功能">
            <column name="id" type="bigint" remarks="主键id">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_sec_menu_function"/>
            </column>
            <column name="menu_id" type="bigint" remarks="菜单ID">
                <constraints nullable="false"/>
